{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://resq-admin.onrender.com/api/:path*"
    },
    {
      "source": "/wallet/:path*",
      "destination": "https://resq-wallet.onrender.com/wallet/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 